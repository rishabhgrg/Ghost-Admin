<section class="gh-canvas">
    <GhCanvasHeader class="gh-canvas-header">
        <h2 class="gh-canvas-title" data-test-screen-title>
            Dashboard
        </h2>
    </GhCanvasHeader>

    <section class="view-container">
        {{#if (and this.session.user.isOwner (not this.feature.launchComplete))}}
            <LinkTo @route="launch">Launch site - Finish setup</LinkTo>
        {{/if}}
    </section>
    <section class="view-container" style="width:700px;background:#fdfdfd;box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;padding-bottom:0">
        <section class="gh-members-chart" style="background: white;padding: 0 12px">
            <GhMembersChart @nightShift={{feature "nightShift"}} @showSummary={{false}} @chartType="counts" />
        </section>
    </section>
</section>